# LLM Configuration
RESUME_TAILOR_LLM_PROVIDER=ollama
RESUME_TAILOR_LLM_MODEL=llama3.1:8b
RESUME_TAILOR_LLM_BASE_URL=http://localhost:11434
RESUME_TAILOR_LLM_TEMPERATURE=0.3
RESUME_TAILOR_LLM_MAX_TOKENS=4000

# Paths
RESUME_TAILOR_BASE_RESUME_PATH=examples/base_resume.yaml
RESUME_TAILOR_STATIC_SECTIONS_PATH=examples/static_sections.yaml
RESUME_TAILOR_OUTPUT_DIR=./output

# RenderCV
RESUME_TAILOR_RENDERCV_THEME=engineeringresumes

# Logging
RESUME_TAILOR_LOG_LEVEL=INFO
RESUME_TAILOR_GEMINI_API_KEY="your_api_key_here"
